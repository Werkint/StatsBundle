{
    "name":        "werkint/stats-bundle",
    "type":        "symfony-bundle",
    "description": "Stats module",
    "license":     "GPL-3.0+",
    "authors":     [
        {
            "name":  "Bogdan Yurov",
            "email": "bogdan@yurov.me"
        }
    ],
    "require":     {
        "symfony/framework-bundle": "*",

        "werkint/framework-extra-bundle": "~1.1.0",
        "werkint/cache-bundle":           "0.1.x-dev",

        "php": ">=5.5"
    },
    "require-dev": {
        "werkint/webapp-bundle": "0.1.x-dev"
    },
    "suggest":     {
        "werkint/webapp-bundle": "Support for Twig extension",
        "twig/twig":             "Twig function for displaying stats in template"
    },
    "autoload":    {
        "psr-4": {
            "Werkint\\Bundle\\StatsBundle\\":        "src/",
            "Werkint\\Bundle\\StatsBundle\\Tests\\": "tests/"
        }
    },
    "extra":       {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },

    "minimum-stability": "dev"
}